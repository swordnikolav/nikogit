# get youtube Atom feed: curl -s -L 'https://www.youtube.com/$user/gocoding/videos' | sfeed_web | cut -f 1

#directorio donde se alojaran los feeds
sfeedpath="/media/datos/varios/sfeed/tfeeds"

#torrent site url
url="https://torrentgalaxy.to"

# list of feeds to fetch:
feeds() {
  # feed <name> <feedurl> [basesiteurl] [encoding]
  # Indivious Linux feeds
  
  feed "P.TorrentGalaxy" ""$url"/rss.php?cat=6"
  feed "S.TorrentGalaxy" ""$url"/rss.php?cat=41"
  feed "M.TorrentGalaxy" ""$url"/rss.php?cat=42"
  feed "F.TorrentGalaxy" ""$url"/rss.php?search="
}
