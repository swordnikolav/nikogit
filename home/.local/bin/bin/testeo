#!/bin/sh
LIB_DIR="/usr/lib64/yambar"

if [ ! -d "$LIB_DIR" ]; then
    echo "Error: No se encuentra el directorio de módulos en $LIB_DIR"
    exit 1
fi

echo "Módulos instalados:"
ls "$LIB_DIR"

echo "\nVerificando si hay librerías rotas en los módulos:"
ldd "$LIB_DIR"/module_*.so | grep "not found"
